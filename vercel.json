{
    "version": 2,
    "builds": [{
        "src": "dist/main.js",
        "use": "@vercel/node",
        "config": {
            "vercel-build": "vercel-build"
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
    }]
}