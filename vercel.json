{
    "version": 2,
    "builds": [{
            "src": "src/main.ts",
            "use": "@vercel/node"
        },
        {
            "src": "prisma/schema.prisma",
            "use": "prisma"
        }
    ],
    "routes": [{
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
    }]
}